    "neverthrow": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "rxjs": "^7.8.1",
    "urql": "^4.1.0",
    "vite-tsconfig-paths": "^5.0.1"
